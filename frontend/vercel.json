{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/f1-app/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/((?!.*\\.(js|css|png|jpg|jpeg|svg|ico|json|txt|woff2|woff|ttf|eot)).*)",
      "dest": "/index.html"
    }
  ]
}
