{
  "name": "f1-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:cicd": "npx ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "build:browser": "ng build --configuration production",
    "build:server": "ng run f1-app:server:production",
    "build:ssr": "npm run build:browser && npm run build:server",
    "serve:ssr": "node dist/f1-app/server/main.js",
    "build:express": "tsc -p tsconfig.express.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.11",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.11",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.12",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@nguniversal/express-engine": "^7.0.2",
    "express": "^4.21.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.12",
    "@angular/cli": "^19.2.12",
    "@angular/compiler-cli": "^19.2.0",
    "@types/express": "^4.17.22",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.17",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.7.2"
  }
}
